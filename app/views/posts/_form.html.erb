<%= form_for :post, url: posts_path, html: { multipart: true } do |f| %>

  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :paragraph %>
    <%= f.text_field :paragraph_one %>
  </div>

  <div class="field">
    <%= f.text_field :paragraph_two %>
  </div>

  <div class="filed">
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div>

  <div>
    <%= f.submit %>
  </div>
<% end %>
